{
  "name": "px-ui",
  "version": "0.3.0",
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "do-clean-install": "rm -rf node_modules && pnpm -r exec rm -rf node_modules && pnpm i",
    "build": "pnpm -r run build",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "storybook": "storybook dev -p 6969 --no-open",
    "build-storybook": "storybook build",
    "dev": "concurrently \"pnpm run watch:styles\" \"pnpm run storybook\"",
    "prepare": "husky install",
    "gen:component": "plop --plopfile generators/plopfile.mjs",
    "watch:styles": "npx @tailwindcss/cli -i ./.storybook/tailwind.css -o ./.storybook/index.css --watch"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/builder-vite": "^8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@tailwindcss/cli": "^4.1.5",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/jest-dom": "6.4.1",
    "@testing-library/react": "14.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "10.4.19",
    "change-case": "^5.4.4",
    "concurrently": "^9.1.2",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.3",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "15.5.2",
    "plop": "^4.0.1",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rollup": "4.40.2",
    "storybook": "^8.6.12",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "4.1.5",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.1.3"
  }
}
