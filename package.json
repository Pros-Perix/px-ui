{
  "name": "px-ui",
  "version": "0.3.0",
  "scripts": {
    "do-clean-install": "rm -rf node_modules && pnpm -r exec rm -rf node_modules && pnpm i",
    "start": "pnpm --filter",
    "build": "pnpm -r run build",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "concurrently": "^9.1.2",
    "eslint": "9.26.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.14"
  }
}
