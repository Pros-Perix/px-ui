{
  "name": "@px-ui/forms",
  "version": "1.0.0",
  "description": "Form components built on top of px-ui-core",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css",
    "./dist/index.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "forms",
    "ui",
    "tailwind",
    "design-system"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pros-Perix/px-ui.git",
    "directory": "packages/forms"
  },
  "peerDependencies": {
    "@px-ui/core": "workspace:*",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-hook-form": ">=7.0.0",
    "zod": ">=3.0.0"
  },
  "devDependencies": {
    "react-hook-form": ">=7.0.0",
    "@hookform/resolvers": "^5.1.1",
    "@px-ui/core": "workspace:*",
    "zod": ">=3.0.0",
    "@tanstack/react-query": "^5.90.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
